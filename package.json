{
  "name": "tile_landscape",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "start:mcp-server": "node -r dotenv/config ./node_modules/.bin/tsx src/server.ts",
    "start:mcp-standalone": "node -r dotenv/config ./node_modules/.bin/tsx src/standalone-server.ts",
    "build:mcp-server": "tsc -p tsconfig.mcp.json",
    "test:mcp-client": "node -r dotenv/config ./node_modules/.bin/tsx src/test-mcp-client.ts",
    "test:mcp-standalone": "node -r dotenv/config ./node_modules/.bin/tsx src/test-standalone.ts",
    "mcp:client": "node -r dotenv/config ./node_modules/.bin/tsx src/mcp-client.ts"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/three": "^0.176.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.8.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.4.4",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@google/genai": "^0.13.0",
    "@modelcontextprotocol/sdk": "^1.11.1",
    "chokidar": "^3.5.3",
    "three": "^0.176.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.0"
  }
}
